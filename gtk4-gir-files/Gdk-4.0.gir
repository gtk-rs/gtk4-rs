      </member>
      <member name="cancel" value="3" c:identifier="GDK_TOUCHPAD_GESTURE_PHASE_CANCEL" glib:nick="cancel">
        <doc xml:space="preserve">The gesture was cancelled, all
  changes should be undone.</doc>
      </member>
    </enumeration>
